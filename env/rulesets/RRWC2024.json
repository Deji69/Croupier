{
	/**
	 * Name of ruleset to appear in Croupier.
	 */
	"Name": "RRWC 2024",
	"Rules": {
		/**
		 * Enable 'Elimination', 'Silenced Elimination' and 'Loud Elimination'
		 * Default: false
		 */
		"GenericEliminations": false,
		
		/**
		 * The chance of a kill type being applied to an applicable kill method.
		 * Default: 50
		 */
		"KillTypeChance": 50,

		/**
		 * Allow 'Melee' to be a kill type for melee weapons.
		 * Default: false
		 */
		"MeleeKillTypes": false,

		/**
		 * Allow 'Thrown' to be a kill type for melee weapons.
		 * Default: false
		 */
		"ThrownKillTypes": false,

		/**
		 * Allow vanilla 'Explosive' (with no kill type).
		 * Default: true
		 */
		"AnyExplosives": false,

		/**
		 * Allow 'Impact' to be a kill type for 'Explosive'.
		 * Default: true
		 */
		"ImpactExplosives": true,

		/**
		 * Allow 'Remote' to be a kill type for 'Explosive'.
		 * Default: true
		 */
		"RemoteExplosives": true,

		/**
		 * Allow 'Loud Remote' to be a kill type for 'Explosive'.
		 * Default: false
		 */
		"LoudRemoteExplosives": false,

		/**
		 * Enable the 'Live' (No Target KO) complication.
		 * Default: true
		 */
		"LiveComplications": true,

		/**
		 * Prevent 'Standard' kills from having live complications.
		 * Default: true
		 */
		"LiveComplicationsExcludeStandard": true,

		/**
		 * Percentage chance of spinning a 'Live' complication.
		 * Default: 25
		 */
		"LiveComplicationChance": 25,

		/**
		 * If true, Loud SMG cannot be spun in a spin that already has a large firearm.
		 * Default: true
		 * NOTE: This is unused. The IsLarge tags on methods/variants are now defined in kill-methods.json.
		 */
		"LoudSMGIsLargeFirearm": false,
		
		/**
		 * The maximum number of 'Weapon' methods (excluding explosives) with the IsLarge tag that are allowed in a spin.
		 * Default: 1
		 */
		"MaxLargeFirearms": 1,

		/**
		 * If true, spins can be generated where two or more targets require the same disguise.
		 * Default: false
		 */
		"AllowDuplicateDisguise": false,

		/**
		 * If true, spins can be generated where two or more targets require the same kill method.
		 * Default: false
		 */
		"AllowDuplicateMethod": false,

		/**
		 * If true, 'Suit' will always be chosen as the disguise for a kill.
		 * Default: false
		 */
		"SuitOnly": false,

		/**
		 * If true, spins can be generated with 'Any Disguise' in the disguise condition.
		 * Croupier will validate only the kill method and ignore the disguise in this case.
		 * Default: false
		 */
		"AnyDisguise": false,

		/**
		 * The per-target tags that Croupier should avoid generating in spins.
		 */
		"Banned": ["Unique", "Banned", "Slow", "Extreme", "Impossible", "Buggy", "EasterEgg"]
	},
	"Tags": {
		/**
		 * "<Target>": { "<Tag>": ["<Method>", "<Disguise>", ...] }
		 * <Target> is usually the initials of the target's name, or "NCR" in the case of Noel Crest.
		 * Use this to tag conditions individually for each target, based on method names, disguise names, complications or method tags (see kill-methods.json)
		 * Special values:
		 *   "Live" - Tag the condition if it has a 'Live' complication.
		 *   "LoudLive" - Tag the condition if the method is a loud firearm kill and the 'Live' complication is set.
		 *   "IsLoud" - Tag the condition if the method is tagged with IsLoud.
		 *   "IsSilenced" - Tag the condition if the method is tagged with IsSilenced.
		 *   "HostileNonRemote" - Tag the condition if the method is not tagged with IsRemote or is tagged with IsLoud or IsImpact, when the disguise is also hostile.
		 *   "IsExplosive" - Tag the condition if the method is tagged with IsExplosive.
		 */
		/** Kalvin Ritter **/
		"KR": {
			"Impossible": ["Electrocution", "Fire", "Injected Poison", "SMG", "Sniper", "ImpactExplosive", "Silenced Assault Rifle", "Silenced Shotgun"]
